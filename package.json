{
  "name": "fm_frontend",
  "version": "1.0.0",
  "description": "Farm Monitor front end client",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --colors -p --mode production",
    "start": "concurrently -n \"WEBPACK,FLASK\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run webpack-dev-server\" \"npm run flask-server\"",
    "webpack-dev-server": "NODE_ENV=debug webpack-dev-server --host 0.0.0.0 --port 2992 --hot --inline --mode development",
    "flask-server": "fm_frontend run --host 0.0.0.0",
    "lint": "eslint \"assets/js/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nstoik/fm_frontend.git"
  },
  "author": "Nelson Stoik",
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">=4"
  },
  "bugs": {
    "url": "https://github.com/nstoik/fm_frontend/issues"
  },
  "homepage": "https://github.com/nstoik/fm_frontend#readme",
  "dependencies": {
    "bootstrap": "^4.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.19.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.3.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "font-awesome-webpack": "git+https://github.com/nstoik/font-awesome-webpack.git#440af2a2efe7ba1779d039556f04538e57bad4bb",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "manifest-revision-webpack-plugin": "^0.4.1",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.21.0",
    "sync-exec": "^0.6.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.2",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  }
}
